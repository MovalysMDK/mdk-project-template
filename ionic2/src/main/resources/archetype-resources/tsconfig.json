        ],
        "mdk-ionic2-lib-core": [
          "jspm_packages/local/mdk-ionic2-lib-core@2.0.0-SNAPSHOT",
          "jspm_packages/mdk/mdk-ionic2-lib-core@master"
        ]
    }
