{
  "compilerOptions": {
    "target": "es5",
    "module": "system",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "outDir": "tmp",
    "sourceRoot": ".",
	"typeCheck": true,
    "paths": {
        "@angular/core": [
          "jspm_packages/npm/@angular/core@2.0.0-rc.4"
        ],
        "@angular/common": [
          "jspm_packages/npm/@angular/common@2.0.0-rc.4"
        ],
        "@angular/compiler": [
          "jspm_packages/npm/@angular/compiler@2.0.0-rc.4"
        ],
        "@angular/http": [
          "jspm_packages/npm/@angular/http@2.0.0-rc.4"
        ],
        "@angular/forms": [
          "jspm_packages/npm/@angular/forms@0.2.0"
        ],
        "@angular/platform-browser-dynamic": [
          "jspm_packages/npm/@angular/platform-browser-dynamic@2.0.0-rc.4"
        ],
        "@angular/platform-browser": [
          "jspm_packages/npm/@angular/platform-browser@2.0.0-rc.4"
        ],
        "ionic-angular": [
          "jspm_packages/npm/ionic-angular@2.0.0-beta.11-201609011853"
        ],
        "ionic-native": [
          "jspm_packages/npm/ionic-native@1.3.21"
        ],
        "rxjs/*": [
